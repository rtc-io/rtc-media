    "gendocs": "$(npm bin)/emu < index.js > README.md"
  },
  "devDependencies": {
    "jshint": "~2.1.4",
    "tape": "~1.0.4",
    "testling": "~1.3.3",
    "emu": "0.0.2"
  },
  "testling": {
    "files": "test/all.js",
    "browsers": {
      "chrome": [ 25, 26, 27, 28, 29, 30 ]
    }
  },
  "contributors": []
}
